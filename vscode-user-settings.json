{
    "[python]": {
        "editor.formatOnType": true,
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "window.zoomLevel": -1,
    "editor.minimap.enabled": false,
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "{git,gitlens}:/**/*.{md,csv,svg}": "default",
        "*.csv": "default",
        "*.md": "default",
        "*.lnk": "default",
        "*.dot": "default",
        "*.rst": "default"
    },
    "workbench.colorCustomizations": {},
    "update.enableWindowsBackgroundUpdates": false,
    "accessibility.dimUnfocused.opacity": 0.8,
    "winopacity.opacity": 249,
    "dimmer.opacity": 30,
    "background.enabled": false,
    "workbench.activityBar.location": "top",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "vscode-office.editorTheme": "Github Dark",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.editor.editorActionsLocation": "titleBar",
    "window.commandCenter": false,
    "codingcopilot.enableAutoCompletions": false,
    "makefile.configureOnOpen": true,
    "workbench.colorTheme": "Solarized Dark",
    "window.customTitleBarVisibility": "windowed",
    "explorer.compactFolders": false,
    "nowcoder.workspaceFolder": "f:\\Edata\\program_language\\git_ln\\nk",
    "inlineChat.experimental.enableZoneToolbar": true,
    "inlineChat.accessibleDiffView": "on",
    "accessibility.verbosity.inlineChat": false,
    "nowcoder.hint.setDefaultLanguage": false,
    "[markdown]": {
        "editor.unicodeHighlight.ambiguousCharacters": false,
        "editor.unicodeHighlight.invisibleCharacters": false,
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        }
    },
    "markdown.copyFiles.destination": {
        "**/*.md": "zFigures/${documentBaseName}/${fileName}"
    },
    "editor.rulers": [
        0, 16, 32, 48, 
        64, 80, 96, 112, 
        128, 144, 160, 176, 
        192, 208, 224, 240, 256
    ],
    "indentRainbow.colors": [
        "rgba(255,255,64,0.07)",
        "rgba(127,255,127,0.07)",
        "rgba(79,236,236,0.07)",
        "rgba(255,127,255,0.07)"
    ]
}